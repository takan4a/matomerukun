{
  "manifest_version": 3,
  "name": "My Bookmarks Extension",
  "version": "1.0.0",
  "description": "独自のブックマークを管理するChrome拡張",
  "action": {
    "default_title": "My Bookmarks"
  },
  "background": {
    "service_worker": "background.js"
  },
  "permissions": ["storage", "tabs"],
  "icons": {
    "16": "icon-16.png",
    "48": "icon-48.png",
    "128": "icon-128.png"
  }
}
